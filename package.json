{
  "name": "nestjs-graphql-orders",
  "version": "0.0.1",
  "description": "⛵ A NestJS application to visualize/query data about e-commerce orders with graphQL",
  "author": "André Silva <andreluis_x@msn.com>",
  "private": true,
  "license": "UNLICENSED",
  "keywords": [
    "nestjs",
    "prisma",
    "prisma client",
    "typescript",
    "graphql"
  ],
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "prisma:migrate:dev:create": "npx prisma migrate dev --create-only",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "prisma:migrate:reset": "npx prisma migrate reset --skip-seed",
    "prisma:migrate:status": "npx prisma migrate status",
    "prisma:pull": "npx prisma db pull",
    "prisma:seed": "npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node prisma/seeders/database.seeder.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.9.0",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^12.0.8",
    "@nestjs/platform-express": "^9.0.0",
    "apollo-server-express": "^3.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "graphql": "^16.7.1",
    "graphql-tools": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@prisma/client": "^5.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "^18.17.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "prisma": "^5.0.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.9.5"
  }
}
